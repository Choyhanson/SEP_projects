@model IEnumerable<ApplicationCore.Models.UserPurchaseResponseModel>
@{ 
    ViewData["Title"] = "Purchases Page";
}

<div class="rounded">
    <div class="container-fluid">
        <div class="row">
            @foreach (var movie in Model)
            {
            <div class="col-xl-2 col-sm-4 col-lg-3">
                @*<partial name="_MoviePurchasedCard" model="movie" />*@
                @await Html.PartialAsync("_MoviePurchasedCard", movie) 

            </div>
            }
        </div>
    </div>
</div>